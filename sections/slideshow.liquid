<style>
  /* LAYOUT SHIFT FIX: Reserve space before images load */
  .slideshow__media {
    position: relative;
    overflow: hidden;
  }
  
  /* Calculate proper aspect ratios based on actual images */
  {% for block in section.blocks %}
    {% if block.settings.image and block.settings.mob_banner %}
      @media screen and (min-width: 768px) {
        #Slide-{{ section.id }}-{{ forloop.index }} .slideshow__media::before {
          content: '';
          display: block;
          padding-bottom: {{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100 }}%;
        }
      }
      
      @media screen and (max-width: 767px) {
        #Slide-{{ section.id }}-{{ forloop.index }} .slideshow__media::before {
          content: '';
          display: block;
          padding-bottom: {{ 1 | divided_by: block.settings.mob_banner.aspect_ratio | times: 100 }}%;
        }
      }
    {% endif %}
  {% endfor %}
  
  /* Position images absolutely to prevent shifts */
  .slideshow__media img, .slideshow__media picture {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  /* PERFORMANCE: Critical CSS optimizations for slideshow */
  
  /* Optimize slideshow container for better LCP */
  slideshow-component {
    display: block;
    contain: layout style paint;
  }
  
  .slideshow {
    position: relative;
    overflow: hidden;
  }
  
  /* Critical image optimization */
  .critical-image, .mobile-banner-img {
    transform: translate3d(0,0,0);
    will-change: transform;
    image-rendering: -webkit-optimize-contrast;
    image-rendering: crisp-edges;
  }
  
  /* Optimize mobile/desktop switching */
  .desktop_banner { display: block; }
  .mobile_banner { display: none; }
  
  @media (max-width: 768px) {
    .mobile_banner { 
      display: block !important; 
      visibility: visible !important;
    }
    .desktop_banner { 
      display: none !important; 
    }
  }

  /*   #Slide-template--18201551110451__782f453f-a109-43d1-8e49-de1598cb442d-3 .slideshow__text-wrapper .banner__box{
